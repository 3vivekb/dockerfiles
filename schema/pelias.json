{
  "logger": {
    "level": "debug",
    "timestamp": false
  },
  "esclient": {
    "apiVersion": "5.0",
    "hosts": [{
      "env": "development",
      "protocol": "http",
      "host": "elasticsearch",
      "port": 9200
    }]
  },
  "elasticsearch": {
    "settings": {
      "index": {
        "refresh_interval": "10s",
        "number_of_replicas": "0",
        "number_of_shards": "1"
      }
    }
  }
}
