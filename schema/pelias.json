{
  "logger": {
    "level": "debug",
    "timestamp": false
  },
  "elasticsearch": {
    "settings": {
      "index": {
        "refresh_interval": "10s",
        "number_of_replicas": "0",
        "number_of_shards": "1"
      }
    }
  },
  "esclient": {
    "apiVersion": "5.0",
    "keepAlive": true,
    "requestTimeout": "120000",
    "maxRetries": "3",
    "deadTimeout": "10000",
    "maxSockets": "100",
    "sniffOnStart": false,
    "hosts": [{
      "env": "development",
      "protocol": "http",
      "host": "elasticsearch",
      "port": 9200
    }]
  }
}
