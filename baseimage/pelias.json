{
  "logger": {
    "level": "debug",
    "timestamp": false
  },
  "esclient": {
    "apiVersion": "5.0",
    "hosts": [{
      "env": "development",
      "protocol": "http",
      "host": "elasticsearch",
      "port": 9200
    }]
  },
  "elasticsearch": {
    "settings": {
      "index": {
        "refresh_interval": "10s",
        "number_of_replicas": "0",
        "number_of_shards": "1"
      }
    }
  },
  "api": {
    "textAnalyzer": "libpostal",
    "services": {
      "pip": {
        "url": "http://pip:4200"
      },
      "placeholder": {
        "url": "http://interpolation.wiz.co.nz"
      }
    }
  },
  "interpolation": {
    "client": {
      "adapter": "http",
      "host": "http://interpolation.wiz.co.nz",
      "timeout": 2500
    }
  },
  "language": {
    "client": {
      "adapter": "http",
      "host": "http://placeholder:4100",
      "timeout": 2500
    }
  },
  "imports": {
    "adminLookup": {
      "enabled": true
    },
    "geonames": {
      "datapath": "/data/geonames",
      "countryCode": "ALL"
    },
    "openstreetmap": {
      "download": [
        {
          "sourceURL": "https://s3.amazonaws.com/metro-extracts.mapzen.com/portland_oregon.osm.pbf"
        }
      ],
      "leveldbpath": "/tmp",
      "datapath": "/data/openstreetmap",
      "import": [{
        "type": { "node": "osmnode", "way": "osmway" },
        "filename": "portland_oregon.osm.pbf"
      }]
    },
    "openaddresses": {
      "datapath": "/data/openaddresses",
      "files": [ "us/or/portland_metro.csv" ]
    },
    "polyline": {
      "datapath": "/data/polyline",
      "files": [ "extract.0sv" ]
    },
    "whosonfirst": {
      "datapath": "/data/whosonfirst",
      "importVenues": false,
      "importPostalcodes": true,
      "importPlace": "101715829",
      "api_key": "mapzen-fepXwQF"
    }
  }
}
